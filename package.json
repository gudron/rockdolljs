  "dependencies": {
    "backbone": "^1.3.3",
    "lodash": "^3.10.1",
    "moment": "^2.18.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "lodash": "^3.10.1",
    "minimist": "^1.2.0",
    "optimist": "^0.6.1",
    "recursive-readdir": "^2.1.1",
    "requestify": "^0.2.5",
    "svgstore-cli": "^1.1.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
