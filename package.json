  "description": "RockdollJS is library of UI modules for MarrionetJS framework",
  "main": "lib/rockdoll.js",
  "scripts": {
    "webpack:build:prod": "./node_modules/.bin/webpack --progress --colors --optimize-minimize true --config webpack.config.js --output-library-target umd",
    "webpack:build:dev": "./node_modules/.bin/webpack --progress --colors --devtool source-map --config webpack.config.js  --output-library-target umd"
